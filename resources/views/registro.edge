<body class="hold-transition register-page">
<div class="register-box">
  <div class="register-logo">
    <a href="../../index2.html"><b>Clien</b>Went</a>
  </div>

  <div class="card">
    <div class="card-body register-card-body">
      <p class="login-box-msg">Registrarse en ClienWent</p>

      <form action="/registro" method="post">
        <div class="input-group mb-3">
          <input type="text" name="nombres" class="form-control" value="{{ old('nombres', '') }}" placeholder="Nombre y Apellido">
          {{ elIf('<span>$self</span>', getErrorFor('nombres'), hasErrorFor('nombres')) }}
          <div class="input-group-append">
            <div class="input-group-text">
              <span class="fas fa-user"></span>
            </div>
          </div>
        </div>
        <div class="input-group mb-3">
          <input name="correo" class="form-control" value="{{ old('correo', '') }}" placeholder="Correo">
          {{ elIf('<span>$self</span>', getErrorFor('email'), hasErrorFor('email')) }}
          <div class="input-group-append">
            <div class="input-group-text">
              <span class="fas fa-envelope"></span>
            </div>
          </div>
        </div>
        <div class="input-group mb-3">
          <input type="password" name="contrasena" class="form-control" placeholder="Contraseña">
          {{ elIf('<span>$self</span>', getErrorFor('contrasena'), hasErrorFor('contrasena')) }}

          <div class="input-group-append">
            <div class="input-group-text">
              <span class="fas fa-lock"></span>
            </div>
          </div>
        </div>
        <div class="input-group mb-3">
          <input type="password" name="contrasena_confirmar" class="form-control" placeholder="Vuelva a escribir la contraseña">
          <div class="input-group-append">
            <div class="input-group-text">
              <span class="fas fa-lock"></span>
            </div>
          </div>
        </div>
        <div class="row">
          <!-- /.col -->
          <div class="col-md-12">
            <button type="submit" class="btn btn-primary btn-block">Registrarse</button>
          </div>
          <!-- /.col -->
        </div>
      </form>

      <div class="social-auth-links text-center">
        <p>- O -</p>
        <a href="#" class="btn btn-block btn-primary">
          <i class="fab fa-facebook mr-2"></i>
          Regístrese con Facebook
        </a>
        <a href="#" class="btn btn-block btn-danger">
          <i class="fab fa-google-plus mr-2"></i>
          Regístrese con Google+
        </a>
      </div>

      <a href="/iniciars" class="text-center">Ya poseo con una cuenta</a>
    </div>
    <!-- /.form-box -->
  </div><!-- /.card -->
</div>
<!-- /.register-box -->

<script src="/assets/plugins/jquery/jquery.min.js"></script>
